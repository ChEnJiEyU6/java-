package banking;
public class Account {
  private double   balance;
  public Account(Stringbal) {
    balance=bal;
  }
  public double getBalance() {
    return balance;
  }
  public boolean deposit(doubleamount) {
  balance=balance+amount;
  return true;
  }
  public boolean withdraw(double amount) {
    boolean result=true;
    if(balance<amount) {
      result=false;
    } else {
      balance=balance-amount;
    }
    return result;
  }
}